{
    "configurations": [
        {
            "showDevDebugOutput": "parsed",
            "cwd": "${workspaceRoot}",
            "executable": "./build/PDMS.elf",
            "name": "Debug STM32",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "preLaunchTask": "Build STM",
            "device": "stm32l496",
            "configFiles": [
                "openocd.cfg"
            ],
            "svdFile": "STM32L4R9.svd"
        },
        {
            "showDevDebugOutput": "parsed",
            "cwd": "${workspaceRoot}",
            "executable": "./build/PDMS.elf",
            "name": "Attach STM32",
            "request": "attach",
            "type": "cortex-debug",
            "servertype": "openocd",
            "preLaunchTask": "Build STM",
            "device": "stm32l496",
            "configFiles": [
                "openocd.cfg"
            ],
            "svdFile": "STM32L4R9.svd"
        },
        {
            "showDevDebugOutput": "parsed",
            "cwd": "${workspaceRoot}",
            "executable": "./build/PDMS.elf",
            "name": "Debug STM32 with Live Expressions",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "preLaunchTask": "Build STM",
            "device": "stm32l496",
            "swoConfig":
            {
                "enabled":  true,
                "cpuFrequency": 80000000,
                "swoFrequency": 200000,
                "source": "probe",
                "decoders": [
                {
                    "label": "ITM port 0 output",
                    "type": "console",
                    "port": 0,
                    "showOnStartup": true,
                    "logfile": "./fan_log.txt",
                    "encoding": "ascii"
                }
                ]
            },
            "configFiles": [
                "openocd.cfg"
            ],
            "svdFile": "STM32L4x6.svd",
            "liveWatch": {
                "enabled": true,
                "samplesPerSecond": 20
            }
        },
    ]
}